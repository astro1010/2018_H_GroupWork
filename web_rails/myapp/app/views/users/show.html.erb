<% provide(:title, @user.user_name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <% if @user.image_name? %>
          <%= image_tag @user.image_name.thumb.url %>
        <% end %>
        <%= @user.user_name %>
      </h1>
    </section>
  </aside>
</div>